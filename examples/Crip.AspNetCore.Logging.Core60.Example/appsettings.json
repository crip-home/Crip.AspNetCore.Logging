{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Crip.AspNetCore.Logging.RequestLoggingMiddleware": "Fatal",
        "Crip.AspNetCore.Logging.RequestLoggingMiddleware.WeatherForecast": "Verbose",
        "Crip.AspNetCore.Logging.RequestLoggingMiddleware.HomeRouteName": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "ApplicationName"
    ],
    "Properties": {
      "Application": "Net6.0-Example"
    }
  },
  "AllowedHosts": "*"
}
